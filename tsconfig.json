{
  "compilerOptions": {
    "module": "CommonJS",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "composite": true,
    "sourceMap": true,
    "declaration": true,
    "target": "ES6",
    "esModuleInterop": true,
    "lib": [
      "dom",
      "dom.iterable"
    ]
  },
}
