{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "ESNext",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "composite": true,
    "sourceMap": true,
    "declaration": true,
    "target": "ESNext",
    "esModuleInterop": true,
    "lib": [
      "dom",
      "dom.iterable"
    ]
  },
}
